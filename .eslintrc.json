{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": ["error", "always-multiline"],
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "quotes": "off"
  }
}
